                  <property name="styleSheet">
                   <string notr="true">/* === Main Window &amp; Scroll Area === */
QScrollArea {
    border: none;
}

/* === General Question Box Style === */
/* This targets any QGroupBox whose name ENDS WITH &quot;GroupBox&quot; */
QGroupBox[objectName$=&quot;GroupBox&quot;] {
    background-color: #f7f7f7; /* Light grey background */
    border-left: 4px solid #26a69a; /* Teal left border */
    border-radius: 2px;
    margin-bottom: 10px; /* Space between question boxes */
    padding: 15px; /* Internal spacing */

    /* These lines hide the other borders to blend in */
    border-top: 1px solid #f0f0f0;
    border-right: 1px solid #f0f0f0;
    border-bottom: 1px solid #f0f0f0;
}

/* === General Question Label Style === */
/* This targets any QLabel whose name ENDS WITH &quot;QuestionLabel&quot; */
QLabel[objectName$=&quot;QuestionLabel&quot;] {
    color: #00796B;
    font-size: 12pt;
    font-weight: bold;
    background-color: transparent; /* Changed from f7f7f7 */
    margin-bottom: 5px;
}

/* === General Radio Button Style === */
/* This styles all QRadioButtons */
QRadioButton {
    font-size: 11pt;
    color: #333333; /* Dark grey text */
    spacing: 8px; /* Space between the circle and the text */
    background-color: transparent; /* Changed from f7f7f7 */
}

/* This styles the round indicator itself */
QRadioButton::indicator {
    width: 14px;
    height: 14px;
}

/* === General Text Input Style === */
/* This targets any QLineEdit whose name ENDS WITH &quot;Textbox&quot; */
QLineEdit[objectName$=&quot;Textbox&quot;] {
    background-color: white;
    border: 1px solid #2D7A6C; /* Teal border */
    border-radius: 4px;
    padding: 5px;
    max-width: 150px;
}

/* === Base Style (for BOTH buttons) === */
/* Targets any QPushButton whose name starts with &quot;ClinicalHistory&quot; */
QPushButton[objectName^=&quot;ClinicalHistory&quot;] {
    /* Shape &amp; Size */
    min-height: 35px; /* Set your desired height */
    border-radius: 17px; /* This MUST be half of min-height */
    border: none;
    
    /* Font */
    font-size: 14px;
    font-weight: bold;
    
    /* Spacing */
    padding-left: 20px;
    padding-right: 20px;
}

/* === &quot;Get my results&quot; Button (Teal) === */
QPushButton#ClinicalHistoryGetResultsButton {
    background-color: #5BC8B6; /* Teal background */
    color: white;
}
QPushButton#ClinicalHistoryGetResultsButton:hover {
    background-color: #6ED4C4; /* Lighter teal */
}
QPushButton#ClinicalHistoryGetResultsButton:pressed {
    background-color: #4AAE9E; /* Darker teal */
}

/* === &quot;Previous&quot; Button (Gray) === */
QPushButton#ClinicalHistoryPreviousButton {
    background-color: #B0B0B0; /* Gray background */
    color: #2D7A6C; /* Dark teal text */
}
QPushButton#ClinicalHistoryPreviousButton:hover {
    background-color: #C0C0C0; /* Lighter gray */
}
QPushButton#ClinicalHistoryPreviousButton:pressed {
    background-color: #A0A0A0; /* Darker gray */
}</string>
